import {model, property} from '@loopback/repository';

<%_
const _referencedModels = locals.referencedModels || [];
const _properties = locals.properties || [];
_referencedModels.forEach(m => {
%>
import {<%- m.className %>} from './<%- m.fileName.substring(0, m.fileName.length -3) %>';
<%_
});
-%>
/**
 * The model class is generated from OpenAPI schema - <%- name %>
 */
@model({name: '<%- name %>'})
export class <%- className %> {
  constructor() {}

  <%_ for (const p of _properties) { -%>
  /**
   * <%- p.description %>
   */
  <%- p.decoration %>
  <%- p.signature %>

  <%_ } -%>
}

